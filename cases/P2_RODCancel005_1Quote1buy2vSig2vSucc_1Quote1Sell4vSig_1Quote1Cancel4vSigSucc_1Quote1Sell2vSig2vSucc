QuoteSvr.Config.SetProviderNum("1");
QuoteSvr.Provider0.WaitConnectFromClient();
QuoteSvr.Provider0.SendResultForConnect("SUCC");  # "SUCC" or "FAIL" or NULL

QuoteSvr.Provider0.WaitLoginFromClient("account=test|password=xxxxx");
QuoteSvr.Provider0.SendResultForLogin("SUCC");    # "SUCC" or "FAIL" or NULL

QuoteSvr.Provider0.WaitSubscribeQuoteFromClient("IF1504");
QuoteSvr.Provider0.SendRspForSubscribe("file=/home/cuiju/att/data/four_quote.dat|interval=500000");

QuoteSvr.Provider0.Sleep(1);  #unit: microsecond
QuoteSvr.Provider0.WaitResultOfSendQuote();  
QuoteSvr.Provider0.DisconnectClient();

Exchange.Config.SetClientNum("1");
Exchange.Client0.WaitConnectFromClient();
Exchange.Client0.SendResultForConnect("SUCC");

Exchange.Client0.WaitLoginFromClient("account=test|password=xxxxx");
Exchange.Client0.SendResultForLogin("SUCC");

Exchange.Client0.WaitReqOrderInsert("orderID=0|instr=ROD|direction=buy|vol=2|price=3838|CombOffsetFlag=Open");
Exchange.Client0.SendOnRtnOrder("orderID=0|SubmitStatus=InsertSubmitted|OrderStatus=Unknown|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=0|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=0|SubmitStatus=Accepted|OrderStatus=PartTradedQueueing|VolumeTraded=2|VolumeTotal=0");
Exchange.Client0.SendOnRtnTrade("orderID=0|vol=2|price=3838");

Exchange.Client0.WaitReqOrderInsert("orderID=1|instr=ROD|direction=sell|vol=2|price=3839|CombOffsetFlag=CloseToday");
Exchange.Client0.WaitReqOrderInsert("orderID=2|instr=ROD|direction=sell|vol=2|price=3839|CombOffsetFlag=Open");

Exchange.Client0.SendOnRtnOrder("orderID=1|SubmitStatus=InsertSubmitted|OrderStatus=Unknown|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=2|SubmitStatus=InsertSubmitted|OrderStatus=Unknown|VolumeTraded=0|VolumeTotal=2");

Exchange.Client0.SendOnRtnOrder("orderID=1|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=2|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");

Exchange.Client0.WaitReqOrderAction("cancelID=0|orderID=1");
Exchange.Client0.WaitReqOrderAction("cancelID=1|orderID=2");

Exchange.Client0.SendOnRtnOrder("orderID=1|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=2|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");

Exchange.Client0.SendOnRtnOrder("orderID=1|SubmitStatus=Accepted|OrderStatus=Canceled|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=2|SubmitStatus=Accepted|OrderStatus=Canceled|VolumeTraded=0|VolumeTotal=2");

Exchange.Client0.WaitReqOrderInsert("orderID=3|instr=ROD|direction=sell|vol=2|price=3841|CombOffsetFlag=CloseToday");
Exchange.Client0.SendOnRtnOrder("orderID=3|SubmitStatus=InsertSubmitted|OrderStatus=Unknown|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=3|SubmitStatus=Accepted|OrderStatus=NoTradeQueueing|VolumeTraded=0|VolumeTotal=2");
Exchange.Client0.SendOnRtnOrder("orderID=3|SubmitStatus=Accepted|OrderStatus=PartTradedQueueing|VolumeTraded=2|VolumeTotal=0");
Exchange.Client0.SendOnRtnTrade("orderID=3|vol=2|price=3841");

Exchange.Client0.DisconnectClient();
